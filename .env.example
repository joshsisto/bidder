# Auction Bot Configuration (.env.example)
# Copy this file to .env and update the values

# Network Configuration
HOME_IP=127.0.0.1
ENABLE_VPN_CHECK=True
HEADLESS_BROWSER=True
NETWORK_TIMEOUT=60000

# Google Search API
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CX=your_google_custom_search_id
USE_GOOGLE_API=True

# Amazon Search
ENABLE_AMAZON_SEARCH=False

# OCR and Object Detection
OBJECT_DETECTION_ENABLED=True
PRODUCT_SEARCH_ENABLED=True
CLOUD_VISION_ENABLED=False
# Override Tesseract path if needed
# TESSERACT_PATH=/custom/path/to/tesseract

# LLM Configuration (OpenRouter)
OPENROUTER_ENABLED=False
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=anthropic/claude-3-opus-20240229

# Custom URL (if not using default)
# AUCTION_URL=https://www.example.com/auction/123